{
  "name": "code-agent",
  "version": "1.0.0",
  "description": "HTTP wrapper for OpenAI Codex CLI",
  "main": "src/server.js",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:old": "node src/server.js",
    "dev": "nest start --watch",
    "docker:build": "docker build -t codex-wrapper .",
    "docker:run": "docker run -p 3002:3002 -e OPENAI_API_KEY=$OPENAI_API_KEY codex-wrapper"
  },
  "keywords": [
    "codex",
    "openai",
    "cli",
    "wrapper",
    "http"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/platform-express": "^10.4.20",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.20",
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.24",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
